<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Player detail</title>
</head>
<body>
    <ul>
        <% player.forEach(function(p){ %>
            <li>
                <h5>Name  : <%= p.name %> </h5>
                <h5> Country <%= p.country %></h5>
                
                <span style="border: 1px solid black;background-color: crimson; width: 300px; height: 300px;"></span>
            </li>
        <% }) %>
        
    </ul>
</body>
</html>